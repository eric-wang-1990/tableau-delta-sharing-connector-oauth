  "tableau-version": {
    "min": "2022.4"
  },
  "vendor": {
    "name": "Databricks",
    "support-link": "https://databricks.com",
    "email": "support@vendor-name.com"
  },
  "permission": {
    "api": {
      "https://*.usgs.gov/": [
